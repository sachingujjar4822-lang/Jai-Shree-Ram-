<!DOCTYPE html>
<html lang="hi">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Mock Test 19 - Army Agniveer GD</title>
  <style>
    body { font-family: Arial, sans-serif; margin: 20px; background: #f9f9f9; }
    .container { max-width: 800px; margin: auto; background: #fff; padding: 20px; border-radius: 8px; box-shadow: 0px 0px 10px #aaa; }
    h2 { text-align: center; }
    .question { margin: 20px 0; font-size: 18px; }
    .options label { display: block; margin: 8px 0; padding: 10px; background: #f0f0f0; border-radius: 5px; cursor: pointer; }
    .btns { display: flex; justify-content: space-between; margin-top: 20px; }
    button { padding: 10px 20px; border: none; border-radius: 5px; cursor: pointer; font-size: 16px; }
    .next { background: #3498db; color: white; }
    .back { background: #95a5a6; color: white; }
    .submit { background: #2ecc71; color: white; width: 100%; margin-top: 20px; }
    .timer { font-size: 20px; font-weight: bold; text-align: right; color: red; }
    .result { margin-top: 20px; padding: 15px; background: #ecf0f1; border-radius: 5px; }
    .wrong { color: red; }
    .correct { color: green; }
  </style>
</head>
<body>
  <div class="container">
    <div class="timer" id="timer">60:00</div>
    <h2>Mock Test 19</h2>
    <div id="quiz"></div>
    <div class="btns">
      <button class="back" onclick="prevQuestion()">⬅ Back</button>
      <button class="next" onclick="nextQuestion()">Next ➡</button>
    </div>
    <button class="submit" onclick="submitQuiz()">Submit Test</button>
    <div id="result"></div>
  </div>

  <script>
    // Timer
    let time = 60 * 60;
    const timerEl = document.getElementById("timer");
    setInterval(() => {
      if (time > 0) {
        time--;
        let m = String(Math.floor(time / 60)).padStart(2, "0");
        let s = String(time % 60).padStart(2, "0");
        timerEl.textContent = `${m}:${s}`;
      }
    }, 1000);

    // Questions (50 total — GK heavy; UPSC Pre + SSC CGL mix)
    const questions = [
      // ---- MATHS (1–13) ----
      { question: "प्रश्न 1: 1250 का 18% = ?", options: ["200", "212.5", "225", "240"], answer: "225" },
      { question: "प्रश्न 2: √1521 का मान क्या है?", options: ["37", "38", "39", "41"], answer: "39" },
      { question: "प्रश्न 3: यदि x + 1/x = 5, तो x² + 1/x² = ?", options: ["21", "22", "23", "24"], answer: "23" },
      { question: "प्रश्न 4: ₹20000 पर 10% वार्षिक दर से 2 वर्ष का चक्रवृद्धि ब्याज (वार्षिक) ?", options: ["₹3800", "₹4000", "₹4200", "₹4400"], answer: "₹4200" },
      { question: "प्रश्न 5: एक ट्रेन 90 किमी/घं से 50 मिनट चली। दूरी = ?", options: ["70 किमी", "72 किमी", "75 किमी", "78 किमी"], answer: "75 किमी" },

      { question: "प्रश्न 6: A:B = 3:5 और B:C = 4:7 हैं। कुल ₹1340 में C का भाग?", options: ["₹560", "₹580", "₹700", "₹720"], answer: "₹700" },
      { question: "प्रश्न 7: 80 L मिश्रण में दूध:पानी = 5:3 है। अनुपात 3:2 करने हेतु पानी और कितना मिलाएँ?", options: ["3 L", "3.33 L", "4 L", "5 L"], answer: "3.33 L" },
      { question: "प्रश्न 8: HCF=12 और LCM=420 हैं। एक संख्या 60 है, दूसरी = ?", options: ["72", "80", "84", "90"], answer: "84" },
      { question: "प्रश्न 9: 18, 25, 33, x, 44 का औसत 30 है। x = ?", options: ["27", "29", "30", "31"], answer: "30" },
      { question: "प्रश्न 10: क्रमिक छूट 20% और 10% के बराबर एकल छूट = ?", options: ["28%", "29%", "30%", "31%"], answer: "28%" },

      { question: "प्रश्न 11: पाइप A 12 घंटे और B 18 घंटे में टंकी भरते हैं। साथ मिलकर समय?", options: ["7 घंटा", "7 घंटा 12 मिनट", "7 घंटा 30 मिनट", "8 घंटा"], answer: "7 घंटा 12 मिनट" },
      { question: "प्रश्न 12: नाव की स्थिर जल गति 12 किमी/घं, धारा 3 किमी/घं। 27 किमी अपस्ट्रीम समय?", options: ["2 घं 30 मिन", "3 घं", "3 घं 15 मिन", "3 घं 30 मिन"], answer: "3 घं" },
      { question: "प्रश्न 13: जिन मूलों का योग 9 और गुणनफल 14 हो, वह द्विघात समीकरण है:", options: ["x²−7x+14=0", "x²−9x+14=0", "x²−14x+9=0", "x²+9x−14=0"], answer: "x²−9x+14=0" },

      // ---- REASONING (14–23) ----
      { question: "प्रश्न 14: एक व्यक्ति 7 किमी उत्तर, 24 किमी पूर्व, 7 किमी दक्षिण गया। प्रारम्भ बिंदु से दूरी?", options: ["14 किमी", "20 किमी", "24 किमी", "26 किमी"], answer: "24 किमी" },
      { question: "प्रश्न 15: A, B का भाई है; B, C की बेटी है; C पुरुष है; D, C की माँ है। D का A से संबंध?", options: ["नानी", "दादी", "मौसी", "चाची"], answer: "दादी" },
      { question: "प्रश्न 16: श्रृंखला: 3, 8, 15, 24, 35, ?", options: ["44", "46", "48", "50"], answer: "48" },
      { question: "प्रश्न 17: कोडिंग: यदि किसी शब्द का मान उसके अक्षरों के स्थानों का योग है, तो DOG = ?", options: ["24", "26", "30", "32"], answer: "26" },
      { question: "प्रश्न 18: 26 जनवरी 2024 को सप्ताह का कौन सा दिन था?", options: ["बुधवार", "गुरुवार", "शुक्रवार", "शनिवार"], answer: "शुक्रवार" },

      { question: "प्रश्न 19: विषम चुनें: 121, 169, 225, 243, 289", options: ["121", "169", "225", "243"], answer: "243" },
      { question: "प्रश्न 20: समानता—Doctor : Hospital :: Judge : ?", options: ["Court", "Law", "Lawyer", "Police"], answer: "Court" },
      { question: "प्रश्न 21: 7:20 पर घड़ी की सुइयों के बीच कोण?", options: ["90°", "95°", "100°", "105°"], answer: "100°" },
      { question: "प्रश्न 22: अक्षर श्रृंखला: A, D, H, M, ?", options: ["Q", "R", "S", "T"], answer: "S" },
      { question: "प्रश्न 23: यदि 'DELHI' के हर अक्षर को +1 शिफ्ट करें तो कोड होगा:", options: ["EFMIJ", "EFMJH", "EFMJI", "EFMJJ"], answer: "EFMIJ" },

      // ---- GK / GS / CURRENT AFFAIRS (24–50) ----
      { question: "प्रश्न 24: प्रस्तावना में भारत को किस रूप में वर्णित किया गया है?", options: ["धर्मनिरपेक्ष राज्य", "सार्वभौम, समाजवादी, धर्मनिरपेक्ष, लोकतांत्रिक गणराज्य", "संघीय राज्य", "एकात्मक राज्य"], answer: "सार्वभौम, समाजवादी, धर्मनिरपेक्ष, लोकतांत्रिक गणराज्य" },
      { question: "प्रश्न 25: जम्मू-कश्मीर से अनुच्छेद 370 कब हटाया गया?", options: ["2014", "2016", "2018", "2019"], answer: "2019" },
      { question: "प्रश्न 26: वित्त आयोग का उल्लेख संविधान के किस अनुच्छेद में है?", options: ["अनु. 280", "अनु. 324", "अनु. 148", "अनु. 368"], answer: "अनु. 280" },
      { question: "प्रश्न 27: CAG (महालेखा परीक्षक) का अनुच्छेद?", options: ["अनु. 148", "अनु. 76", "अनु. 32", "अनु. 165"], answer: "अनु. 148" },
      { question: "प्रश्न 28: GST किस संविधान संशोधन से लागू हुआ?", options: ["97वाँ", "99वाँ", "100वाँ", "101वाँ"], answer: "101वाँ" },

      { question: "प्रश्न 29: मौलिक अधिकार किस भाग में हैं?", options: ["भाग II", "भाग III", "भाग IV", "भाग IVA"], answer: "भाग III" },
      { question: "प्रश्न 30: राज्य के नीति-निदेशक तत्त्व किस भाग में हैं?", options: ["भाग II", "भाग III", "भाग IV", "भाग IVA"], answer: "भाग IV" },
      { question: "प्रश्न 31: रिट ‘हैबियस कॉर्पस’ का उद्देश्य?", options: ["स्वतंत्र गति का अधिकार", "गिरफ्तार व्यक्ति को न्यायालय के समक्ष प्रस्तुत कराना", "सार्वजनिक हित वाद", "संपत्ति अधिकार"], answer: "गिरफ्तार व्यक्ति को न्यायालय के समक्ष प्रस्तुत कराना" },
      { question: "प्रश्न 32: ‘वंदे मातरम्’ के रचयिता कौन हैं?", options: ["रवीन्द्रनाथ टैगोर", "बंकिमचंद्र चट्टोपाध्याय", "सुभाष बोस", "आचार्य चाणक्य"], answer: "बंकिमचंद्र चट्टोपाध्याय" },
      { question: "прश्न 33: भारत के प्रथम राष्ट्रपति कौन थे?", options: ["डॉ. राजेन्द्र प्रसाद", "डॉ. राधाकृष्णन", "जाकिर हुसैन", "वी.वी. गिरी"], answer: "डॉ. राजेन्द्र प्रसाद" },

      { question: "प्रश्न 34: क्षेत्रफल के आधार पर भारत का सबसे बड़ा राज्य?", options: ["मध्य प्रदेश", "महाराष्ट्र", "राजस्थान", "उत्तर प्रदेश"], answer: "राजस्थान" },
      { question: "प्रश्न 35: भारत का सर्वाधिक जनसंख्या वाला राज्य?", options: ["महाराष्ट्र", "बिहार", "पश्चिम बंगाल", "उत्तर प्रदेश"], answer: "उत्तर प्रदेश" },
      { question: "प्रश्न 36: भारत में सबसे लंबी समुद्री तटरेखा किस राज्य की है?", options: ["तमिलनाडु", "आंध्र प्रदेश", "गुजरात", "ओडिशा"], answer: "गुजरात" },
      { question: "प्रश्न 37: विश्व का सबसे ऊँचा पर्वत कौन सा है?", options: ["के2", "धौलागिरि", "कंचनजंघा", "माउंट एवरेस्ट"], answer: "माउंट एवरेस्ट" },
      { question: "प्रश्न 38: भारत की सबसे लंबी नदी (भारत में प्रवाहित लंबाई के आधार पर)?", options: ["गंगा", "ब्रह्मपुत्र", "गोदावरी", "सिंधु"], answer: "गंगा" },

      { question: "प्रश्न 39: कर्क रेखा भारत के कितने राज्यों से गुजरती है?", options: ["6", "7", "8", "9"], answer: "8" },
      { question: "प्रश्न 40: भारतीय थार मरुस्थल मुख्यतः किस राज्य में स्थित है?", options: ["गुजरात", "राजस्थान", "हरियाणा", "मध्य प्रदेश"], answer: "राजस्थान" },
      { question: "प्रश्न 41: भारत का सबसे लंबा बाँध कौन सा है?", options: ["टिहरी बाँध", "भाखड़ा-नांगल", "हिराकुड बाँध", "सरदार सरोवर"], answer: "हिराकुड बाँध" },
      { question: "प्रश्न 42: सौर मंडल का सबसे बड़ा ग्रह?", options: ["शनि", "बृहस्पति", "अरुण", "वरुण"], answer: "बृहस्पति" },
      { question: "प्रश्न 43: विटामिन D की कमी से कौन-सी बीमारी होती है?", options: ["एनीमिया", "रातांधता", "रिकेट्स", "स्कर्वी"], answer: "रिकेट्स" },

      { question: "प्रश्न 44: इंसुलिन का स्राव शरीर के किस अंग से होता है?", options: ["यकृत", "अग्न्याशय", "गुर्दा", "अग्न्याशय का द्वीपिका"], answer: "अग्न्याशय" },
      { question: "प्रश्न 45: बल की SI इकाई क्या है?", options: ["न्यूटन", "जूल", "पास्कल", "वाट"], answer: "न्यूटन" },
      { question: "प्रश्न 46: pH 7 से कम किस प्रकार का घोल दर्शाता है?", options: ["क्षारीय", "अम्लीय", "तटस्थ", "बफर"], answer: "अम्लीय" },
      { question: "प्रश्न 47: मनुष्य की कौन-सी कोशिका सामान्यतः नाभिक-रहित होती है?", options: ["श्वेत रक्त कण", "लाल रक्त कण", "त्वचा कोशिका", "न्यूरॉन"], answer: "लाल रक्त कण" },
      { question: "प्रश्न 48: भारत का पहला परमाणु परीक्षण ‘स्माइलिंग बुद्धा’ किस वर्ष हुआ?", options: ["1966", "1971", "1974", "1998"], answer: "1974" },

      { question: "प्रश्न 49: चंद्रयान-3 ने चंद्रमा पर सफल लैंडिंग कब की?", options: ["15 अगस्त 2023", "23 अगस्त 2023", "2 सितंबर 2023", "5 सितंबर 2023"], answer: "23 अगस्त 2023" },
      { question: "प्रश्न 50: 2024 ग्रीष्मकालीन ओलंपिक किस शहर में आयोजित हुए?", options: ["टोक्यो", "लॉस एंजेलिस", "पेरिस", "ब्रिस्बेन"], answer: "पेरिस" }
    ];

    let current = 0;
    let answers = {};

    function loadQuestion() {
      const q = questions[current];
      const quiz = document.getElementById("quiz");
      quiz.innerHTML = `
        <div class="question"><b>${q.question}</b></div>
        <div class="options">
          ${q.options.map(opt => `
            <label>
              <input type="radio" name="q${current}" value="${opt}" ${answers[current] === opt ? "checked" : ""}>
              ${opt}
            </label>
          `).join("")}
        </div>
      `;
    }

    function nextQuestion() {
      saveAnswer();
      if (current < questions.length - 1) {
        current++;
        loadQuestion();
      }
    }

    function prevQuestion() {
      saveAnswer();
      if (current > 0) {
        current--;
        loadQuestion();
      }
    }

    function saveAnswer() {
      const selected = document.querySelector(`input[name="q${current}"]:checked`);
      if (selected) answers[current] = selected.value;
    }

    function submitQuiz() {
      saveAnswer();
      let score = 0;
      let resultHTML = "<h3>Result</h3>";
      questions.forEach((q, i) => {
        let userAns = answers[i] || "नहीं चुना";
        let correct = userAns === q.answer;
        if (correct) score++;
        resultHTML += `
          <p><b>${q.question}</b><br>
          आपका उत्तर: <span class="${correct ? 'correct' : 'wrong'}">${userAns}</span><br>
          सही उत्तर: <span class="correct">${q.answer}</span></p>
        `;
      });
      resultHTML += `<h3>कुल स्कोर: ${score} / ${questions.length}</h3>`;
      document.getElementById("result").innerHTML = resultHTML;
    }

    loadQuestion();
  </script>
</body>
</html>
```0
